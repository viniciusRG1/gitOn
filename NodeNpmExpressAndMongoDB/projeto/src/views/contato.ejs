    <%- includes('includes/head')%>
    <%- include('include/navBar')%>
    <div class="container">
      <div class="row">
        <div class="col-lg-2"></div>
        <div class="col-lg-8 my-5">
          <h1 class="text-center">agenda</h1>
          <P class="text-center lead">cria e edite seus contatos</P>
          <%- include('includes/messages') %>
          
          <form action="/contato/register" method="POST">
            <input type="hidden" name="_csrf" value="<%= csrfToken %>">
            <div class="form-group">
              <label>nome</label>
              <input type="text" class="form-control" name="nome" value="<%= contato.nome%>">
            </div>
            <div class="form-group">
              <label>sobre nome</label>
              <input type="text" class="form-control" name="sobreNome" value="<%= contato.sobreNome%>">
            </div>
            <div class="form-group">
              <label>email</label>
              <input type="email" class="form-control" name="email" value="<%= contato.email%>">
            </div>
            <div class="form-group">
              <label>telefone</label>
              <input type="tell" class="form-control" name="tell" value="<%= contato.tell%>">
            </div>
          </form>
        </div>
        <div class="col-lg-2"></div>
      </div>
    </div>
    <%- include ('includes') %>